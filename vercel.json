{
  "version": 2,
  "name": "ai-chat-assistant-storybook",
  "buildCommand": "npm run build-storybook",
  "outputDirectory": "storybook-static",
  "installCommand": "npm install",
  "framework": "static",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/storybook-static/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}